[run]
source = lecf
omit = 
    tests/*
    venv/*
    .venv/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
    def setup_logging

[html]
directory = coverage_html_report 